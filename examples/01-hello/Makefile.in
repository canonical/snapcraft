@GL_GENERATE_ERRNO_H_TRUE@	      -e 's|@''EOVERFLOW_VALUE''@|$(EOVERFLOW_VALUE)|g' \
@GL_GENERATE_ERRNO_H_TRUE@	      < $(top_srcdir)/lib/errno.in.h; \
@GL_GENERATE_ERRNO_H_TRUE@	} > $@-t && \
@GL_GENERATE_ERRNO_H_TRUE@	mv $@-t $@
